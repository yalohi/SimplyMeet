@inject IStringLocalizer<SharedResource> Loc

<div class="row gx-0 sticky-top">
	<div class="col">
		<NavLink class="nav btn btn-secondary border-0 rounded-0 p-1 pt-2 w-100 h-100" href="@NavigationConstants.NAV_MATCH_NEW">
			<i class="fas fa-id-card text-info"></i>
			<strong>@Loc["New"]</strong>
		</NavLink>
	</div>
	<div class="col">
		<NavLink class="nav btn btn-secondary border-0 rounded-0 p-1 pt-2 w-100 h-100" href="@NavigationConstants.NAV_MATCH_LIKED">
			<i class="fas fa-plus-square text-success"></i>
			<strong>@Loc["Liked"]</strong>
		</NavLink>
	</div>
	<div class="col">
		<NavLink class="nav btn btn-secondary border-0 rounded-0 p-1 pt-2 w-100 h-100" href="@NavigationConstants.NAV_MATCH_DISLIKED">
			<i class="fas fa-minus-square text-danger"></i>
			<strong>@Loc["Disliked"]</strong>
		</NavLink>
	</div>
	<div class="col">
		<NavLink class="nav btn btn-secondary border-0 rounded-0 p-1 pt-2 w-100 h-100" href="@NavigationConstants.NAV_MATCH_FILTER">
			<i class="fas fa-filter"></i>
			<strong>@Loc["Filter"]</strong>
		</NavLink>
	</div>
</div>